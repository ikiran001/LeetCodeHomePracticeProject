package com.Practice;

import java.util.Arrays;

public class ArrRev {

	public static int[] rev(int[] arr) {

		int[] arrays = new int[arr.length];
		for (int i = arr.length; i >= 0; i--) {

			System.out.println(arr[i]);
		}

		return null;
	}

	public static void main(String[] args) {

		int a[] = { 5, 4, 3, 2, 1 };
		rev(a);
		System.out.println();

	}

}
