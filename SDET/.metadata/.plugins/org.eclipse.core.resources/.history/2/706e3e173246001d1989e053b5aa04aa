package com.Practice;

public class removeduplicatesInt {

	public static int rem(int a[], int l) {
		int j = 0;

		for (int i = 0; i < n - 1; i++) {
			if (a[i] != a[i + 1]) {
				a[j++] = a[i];
			}
		}

		a[j++] = a[n - 1];

		return j;
	}

	public static void main(String[] args) {

		int a[] = { 1, 3, 5, 9, 8, 8, 7, 2, 5, 6 };
		int le = a.length;
		int j = 0;
		j = rem(a, le);

		for (int i = 0; i < j; i++)
			System.out.print(a[i] + " ");
	}

}
