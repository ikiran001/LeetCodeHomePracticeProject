package demo.java;

public class IntCountWithString {

	public static void main(String[] args) {
		String s = "123ganesh123";
		String temp1 = s.replaceAll("[\\D]", "");

		int no = Integer.parseInt(temp1);

		int temp = 0;
		int sum = 0;
		while (no != 0) {
			temp = no % 10;
			no = no / 10;
			sum = sum + temp;
		}
		System.out.println(" Sum : " + sum);

	}

}
