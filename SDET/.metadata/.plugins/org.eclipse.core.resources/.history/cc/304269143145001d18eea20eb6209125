package com.Practice;

public class ArrRev {

	public static int[] rev(int []arr){
		
		for (int i = 0; i < arr.length; i--) {
			
			System.out.println(arr[i]);
		}
		
		return  arr;
	}
	
	
	public static void main(String[] args) {

		int a[]={5,4,3,2,1};
		rev(a);
		System.out.println();
		
		
	}

}
